	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetBlogServlet</servlet-name>
		<url-pattern>/servlet/GetBlogServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HomeServlet</servlet-name>
